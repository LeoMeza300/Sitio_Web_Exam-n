{{> header}}
<div class="container">
    <h1>pintores</h1>
    {{#if pintores}}
        <table class = "table table-dark table-hever"></table>
        <thead>
              <tr>
                  <td>ID</td>
                  <td>Nombre</td>
                  <td>Nacionalidad</td>
                  <td>Corriente</td>
                  <td>Imagen</td>
              </tr>
        </thead>
        <tbody>
        {{#each pintores}}
        <tr>
            <td>{{_id}}</td>
            <td>{{Nombre}}</td>
            <td>{{Nacionalidad}}}</td>
            <td>{{Corriente}}</td>
            <td>{{Pintura}}}</td>

        </tr>
        {{/each}}
        </tbody>
    {{else}}
        <div>No se encuentra pintores</div>
        {{if}}
</div>
{{>footer}}
